<?xml version="1.0" encoding="UTF-8"?>

<presentation apphelp="com.ibm.mbs.doc,searchsol/c_search_solutions_application.html" beanclass="psdi.webclient.beans.servicedesk.SearchBean" id="searchsol" mboname="SOLUTION" resultstableid="results_showlist" version="6.2.3">
	<page id="mainrec">
		<include id="selfServicePageHeader"/>
		<clientarea id="clientarea">
			<section id="nav">
				<appbar id="servreqs"/>
			</section>
			<datasrc beanclass="psdi.webclient.beans.servicedesk.SearchBean" id="search" mboname="SEARCHSOLUTION"/>
			<tabgroup format="carddeck" id="maintabs">
				<tab default="true" id="results" label="List" type="list">
					<section border="true" datasrc="search" id="list_grid1" label="Search Solutions">
						<sectionrow id="list_grid1_1">
							<sectioncol id="list_grid1_1_1_grid1">
								<section id="list_grid1_1_1_grid2">
									<textbox dataattribute="solution" datasrc="results_showlist" defaultfocus="true" id="list_grid1_1_1" inputmode="query"/>
									<textbox dataattribute="description" id="list_grid1_1_2"/>
									<textbox dataattribute="type" datasrc="results_showlist" id="list_grid1_1_3" inputmode="query"/>
								</section>
							</sectioncol>
							<sectioncol id="list_grid1_2_2">
								<section id="list_grid1_2_2_grid2">
									<textbox applink="assetcat" dataattribute="classification" id="list_grid1_1_2_1" inputmode="readonly" lookup="solution" menutype="classificationqbe"/>
									<textbox dataattribute="CLASSSTRUCTURE.description_class" id="list_grid1_1_2_2a" inputmode="readonly" lookup="classification"/>
								</section>
							</sectioncol>
						</sectionrow>
						<buttongroup id="list_grid2_1_1_grid7">
							<pushbutton default="true" id="list_grid2_1_1_grid7_1" label="Find" mxevent="find" targetid="search"/>
							<pushbutton id="list_grid2_1_1_grid7_2" label="Reset" mxevent="clear" targetid="search"/>
						</buttongroup>
					</section>
					<menubar id="querytoolbar"/>
					<table beanclass="psdi.webclient.beans.servicedesk.CreateLogBean" datasrc="results_showlist" id="results_showlist" inputmode="readonly" label="View Solutions" mboname="SOLUTION" startempty="false" startrow="0">
						<tablebody displayrowsperpage="15" filterable="false" filterexpanded="false" id="list_showlist_tablebody">
							<tablecol dataattribute="solution" id="list_showlist_tablebody_2" mxevent="selectrecord" mxevent_desc="Go To %1" type="link"/>
							<tablecol dataattribute="description" id="list_showlist_tablebody_3"/>
							<tablecol applink="assetcat" dataattribute="CLASSSTRUCTURE.HIERARCHYPATH" filterable="false" id="list_showlist_tablebody_5" menutype="classification" sortable="false"/>
							<tablecol dataattribute="type" id="list_showlist_tablebody_6"/>
						</tablebody>
					</table>
				</tab>
				<tab id="main" label="Details" type="insert">
					<section border="true" id="main_grid1">
						<sectionheader id="main_grid1_sectionheader" label="Solution {0}">
							<paramvalues id="main_grid1_sec_params" property="label">
								<paramvalue dataattribute="solution" id="main_grid1_sec_param_1" position="0"/>
							</paramvalues>
							<controlcontainer id="selfserviceheaderlinks">
								<hyperlink align="right" id="main_grid3_row_col1_sec_h_1" image="nav_icon_previous.gif" label="Previous Record" mxevent="PREVIOUS"/>
								<image height="5" id="main_grid3_row_col1_sec_sep_1" imagename="blank.gif" width="10"/>
								<hyperlink align="right" id="main_grid3_row_col1_sec_h_2" image="nav_icon_next.gif" imagealign="right" label="Next Record" mxevent="NEXT"/>
								<image id="main_grid3_row_col1_sec_sep_2" imagename="tablebtn_divider.gif"/>
								<hyperlink align="right" eventvalue="solutionprint.rptdesign" id="main_grid3_row_col1_sec_h_3" image="btn_printView.gif" label="Print View" mxevent="RUNREPORTBYNAME"/>
							</controlcontainer>
						</sectionheader>
						<sectionrow id="main_grid1_1">
							<sectioncol id="main_grid1_1_1_grid1">
								<section id="main_grid1_1_1_grid2">
									<multiparttextbox dataattribute="solution" defaultfocus="true" descdataattribute="description" descinputmode="readonly" id="main_grid1_1_1" longdescreadonly="true"/>
									<multiparttextbox dataattribute="CLASSSTRUCTURE.HIERARCHYPATH" descdataattribute="CLASSSTRUCTURE.DESCRIPTION" descinputmode="readonly" id="main_grid1_1_4"/>
								</section>
							</sectioncol>
							<sectioncol id="main_grid1_1_1_grid1_a">
								<section id="main_grid1_1_1_grid2_a">
									<textbox dataattribute="type" id="main_grid1_1_1_a" inputmode="readonly"/>
								</section>
							</sectioncol>
						</sectionrow>
					</section>
					<section id="solution_grid2" inputmode="readonly" label="Solution Details">
						<richtextviewer columns="70" dataattribute="problemcode_longdescription" id="solution_grid2_0" rows="4" title="Symptom:"/>
						<blankline id="sol_bl1"/>
						<richtextviewer columns="70" dataattribute="fr1code_longdescription" id="solution_grid2_1" rows="4" title="Cause:"/>
						<blankline id="sol_bl2"/>
						<richtextviewer columns="70" dataattribute="fr2code_longdescription" id="solution_grid2_2" rows="4" title="Resolution:"/>
					</section>
					<table beanclass="psdi.webclient.beans.doclinks.DocLinksBean" collapsable="true" collapsed="false" id="attachments" inputmode="readonly" label="Attachments" relationship="DOCLINKS">
						<tablebody displayrowsperpage="5" filterable="true" filterexpanded="false" id="attachments_tablebody">
							<tablecol dataattribute="document" id="attachments_tablebody_2" type="openurl" urlattribute="weburl"/>
							<tablecol dataattribute="docinfo.description" id="attachments_tablebody_3"/>
						</tablebody>
					</table>
					<buttongroup id="qbe_grid2_1_1_grid7" label="Did this solution help you resolve your issue?" labelalign="right">
						<pushbutton id="qbe_grid2_1_1_grid7_1" label="Yes" mxevent="createSR" targetid="results_showlist"/>
						<pushbutton id="qbe_grid2_1_1_grid7_2" label="No - Create a Service Request" mxevent="createNewSR" targetid="results_showlist"/>
						<pushbutton id="qbe_grid2_1_1_grid7_3" label="No - Return to Solution Search" mxevent="returnSolution" targetid="results_showlist"/>
					</buttongroup>
				</tab>
			</tabgroup>
		</clientarea>
		<include id="pageFooter"/>
	</page>

	<dialog beanclass="psdi.webclient.beans.common.ClassificationBean" id="searchmore" inputmode="query" label="More Search Fields">
		<section id="qbe_grid1">
			<multiparttextbox dataattribute="solution" descdataattribute="description" descinputmode="query" desclookup="longdesc" id="qbe_grid1_1" inputmode="query"/>
			<textbox applink="assetcat" dataattribute="CLASSSTRUCTURE.HIERARCHYPATH" id="qbe_grid1_4" inputmode="query" menutype="classificationqbe"/>
			<richtexteditor dataattribute="PROBLEMCODE_LONGDESCRIPTION" id="qbe_grid1_5" inputmode="query" label="Symptom" numcols="75" numrows="4" title="Symptom"/>
			<richtexteditor dataattribute="FR1CODE_LONGDESCRIPTION" id="qbe_grid1_6" inputmode="query" label="Cause" numcols="75" numrows="4" title="Cause"/>
			<richtexteditor dataattribute="FR2CODE_LONGDESCRIPTION" id="qbe_grid1_7" inputmode="query" label="Resolution" numcols="75" numrows="4" title="Resolution"/>
		</section>
		<buttongroup id="qbe_grid5_1_1_grid6">
			<pushbutton default="true" id="qbe_grid5_1_1_grid6_1" label="Find" mxevent="dialogok"/>
			<pushbutton id="qbe_grid5_1_1_grid6_2" label="Clear" mxevent="qbeclear"/>
			<pushbutton id="qbe_grid5_1_1_grid6_3" label="Cancel" mxevent="qbecancel"/>
		</buttongroup>
	</dialog>
</presentation>
